{
  "name": "@alwatr/flight-finder-pwa",
  "version": "0.0.0",
  "description": "Alwatr Flight Finder PWA",
  "type": "module",
  "author": "S. Ali Mihandoost <ali.mihandoost@gmail.com> (https://ali.mihandoost.com)",
  "private": true,
  "engines": {
    "npm": ">=7.0.0"
  },
  "browserslist": "> 0.1%, not dead",
  "repository": {
    "type": "git",
    "url": "https://github.com/AliMD/flight-finder",
    "directory": "pwa"
  },
  "scripts": {
    "b": "yarn build",
    "cb": "run-s clean build",
    "s": "run-s clean build serve",
    "w": "yarn watch",
    "start": "yarn serve",
    "clean": "rimraf build/",
    "build": "run-s build:*",
    "build:ts": "tsc --build --pretty",
    "build:es": "yarn clean && rollup -c",
    "serve": "web-dev-server --config dev-server.mjs",
    "watch": "run-s clean build:ts && run-p watch:ts serve",
    "watch:ts": "yarn build:ts --watch --preserveWatchOutput"
  },
  "dependencies": {
    "@alwatr/element": "^0.22.0",
    "@alwatr/fetch": "^0.22.1",
    "@alwatr/icon": "^0.22.1",
    "@alwatr/logger": "^0.22.0",
    "@alwatr/router": "^0.22.1",
    "@alwatr/signal": "^0.22.1",
    "@ionic/core": "^6.3.8",
    "@webcomponents/webcomponentsjs": "^2.7.0",
    "lit": "^2.4.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@rollup/plugin-babel": "^6.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@web/dev-server": "^0.1.35",
    "npm-run-all": "^4.1.5",
    "rollup": "^3.2.5",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-summary": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-lit-plugin": "^1.2.1",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4"
  }
}
